<ul>
  <% @list.each do |bookmark| %>
    <li class="bookmark" id="bookmark-<%= bookmark.id %>">
      <a href="<%= bookmark.url %>" target="_blank">
        <%= bookmark.title %>
      </a>
      <form action="/bookmarks/edit/<%= bookmark.id %>" method="get">
       <input type="submit" value="Edit" />
     </form>
    </li>
  <% end %>
</ul>
<form action="/add" method="POST">
  <input type="text" name="add_bookmark">
  <input type="text" name="Title">
  <input type="submit" name="add">
</form>

<form action="/delete" method="POST">
  <input type="text" name="delete_bookmark">
  <input type="submit" name="Yeet">
</form>
